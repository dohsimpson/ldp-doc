<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dohsimpson.ldpdoc" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>LDP Doc</name>
    <description>
        Doc Viewer for LDP (The Linux Documentation Project)
    </description>
    <author email="mydohsimpson@gmail.com" href="http://enting.org">
        Doh Simpson
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
